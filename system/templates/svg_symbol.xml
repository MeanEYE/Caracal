<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../schema/template.dtd">
<document>
	<svg
		class="isset($params['class']) ? $params['class'] : false"
		cms:optional="class"
		>
		<use
			href="$params['fallback'] ? '#'.$params['symbol'] : false"
			xlink:href="$params['url'].'#'.$params['symbol']"
			cms:eval="xlink:href"
			cms:optional="href"
			/>
	</svg>
</document>
