<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<nav class="window">
		<cms:var name="$params['link_new']"/>
	</nav>

	<!-- Header -->
	<table class="list" data-height="300">
		<thead>
			<tr>
				<td style="width: 100px;">
					<cms:text constant="column_text_id" module="news"/>
				</td>
				<td style="width: 250px;">
					<cms:text constant="column_title" module="news"/>
				</td>
				<td>
					<cms:text constant="column_options"/>
				</td>
			</tr>
		</thead>

		<tbody>
			<cms:group_list/>
		</tbody>
	</table>

</document>
