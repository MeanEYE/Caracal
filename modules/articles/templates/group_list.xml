<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<nav class="window">
		<cms:var param="link_new"/>
	</nav>

	<!-- List -->
	<table class="list" data-height="300">
		<thead>
			<td style="width: 170px;">
				<cms:text constant="column_text_id" module="articles"/>
			</td>
			<td style="width: 300px;">
				<cms:text constant="column_title" module="articles"/>
			</td>
			<td class="options">
				<cms:text constant="column_options"/>
			</td>
		</thead>

		<tbody>
			<cms:group_list only_visible="0"/>
		</tbody>
	</table>
</document>
