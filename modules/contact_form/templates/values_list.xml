<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<nav class="window">
		<cms:var name="$params['link_new']"/>
		<cms:var name="$params['link_import']"/>
	</nav>

	<!-- Header -->
	<table class="list" data-height="350">
		<thead>
			<td class="column" style="width: 150px;">
				<cms:text constant="column_name" module="contact_form"/>
			</td>
			<td class="column" style="width: 150px;">
				<cms:text constant="column_value" module="contact_form"/>
			</td>
			<td class="options">
				<cms:text constant="column_options"/>
			</td>
		</thead>

		<tbody>
			<cms:list field="$params['field']" cms:eval="field"/>
		</tbody>
	</table>
</document>
