<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<cms:if condition="$params['type'] != 'honey-pot'">
		<tr>
			<td>
				<label class="radio">
					<input
						type="checkbox"
						name="'include_'.$params['name']"
						checked="checked"
						cms:eval="name"
						/>
					<cms:var name="$params['name']"/>
				</label>
			</td>
			<td>
				<input
					type="text"
					name="'header_'.$params['name']"
					style="width: 170px;"
					cms:eval="name"
					/>
			</td>
		</tr>
	</cms:if>
</document>
