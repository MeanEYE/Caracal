<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<form class="submission_details">
		<cms:fields
			submission="$params['id']"
			cms:eval="submission"
			/>

		<div class="button_bar">
			<button
				type="button"
				onclick="$params['button_action']"
				cms:eval="onclick"
				>
				<cms:text constant="close"/>
			</button>
		</div>
	</form>
</document>
