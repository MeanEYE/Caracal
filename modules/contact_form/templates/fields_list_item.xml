<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<tr>
		<td>
			<cms:var name="$params['name']"/>
		</td>
		<td>
			<cms:var name="$params['type']"/>
		</td>
		<td class="options">
			<cms:var param="item_change"/>
			<cms:var param="item_delete"/>

			<cms:if condition="$params['type'] == 'select'">
				<cms:var param="item_values"/>
			</cms:if>
		</td>
	</tr>
</document>
