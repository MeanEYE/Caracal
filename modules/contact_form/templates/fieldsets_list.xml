<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<nav class="window">
		<cms:var param="link_new"/>
	</nav>

	<!-- Header -->
	<table class="list" data-height="300">
		<thead>
			<td style="width: 200px;">
				<cms:text constant="column_name" module="contact_form"/>
			</td>
			<td class="options">
				<cms:text constant="column_options"/>
			</td>
		</thead>

		<tbody>
			<cms:list
				template="fieldsets_list_item.xml"
				local="1"
				form="$params['form']"
				cms:eval="form"
				/>
		</tbody>
	</table>
</document>
