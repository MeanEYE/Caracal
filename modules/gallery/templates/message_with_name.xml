<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<div style="margin-bottom: 10px;">
		<cms:var name="$params['message']"/>
		<div style="padding: 10px; font-weight: bold;">
			<cms:var param="name"/>
