<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<nav class="window">
		<cms:var name="$params['link_new']"/>
	</nav>

	<!-- Header -->
	<div class="scrollable_list">
		<div class="list_header">
			<span class="column" style="width: 250px;">
				<cms:text constant="column_group_name" module="gallery"/>
			</span>
			<span class="column">
				<cms:text constant="column_options"/>
			</span>
		</div>
		<div class="list_content" style="height: 250px">
			<cms:group_list/>
		</div>
	</div>

</document>
