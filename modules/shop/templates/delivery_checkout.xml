<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<a
		href="javascript: void(0);"
		data-value="$params['name']"
		data-custom-interface="$params['custom_interface']"
		data-user-information="$params['user_information']"
		cms:eval="data-value,data-custom-interface,data-user-information"
		>
		<img
			src="$params['small_image']"
			alt="$params['title']"
			cms:eval="src,alt"
			width="100"
			height="28"
			/>
		<cms:var param="title"/>
	</a>
</document>
