<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<nav class="window">
		<cms:var param="link_new"/>
	</nav>

	<!-- Header -->
	<div class="scrollable_list">
		<div class="list_header">
			<span class="column" style="width: 250px;">
				<cms:text constant="column_name" module="shop"/>
			</span>
			<span class="column">
				<cms:text constant="column_options" module="shop"/>
			</span>
		</div>
		<div class="list_content" style="height: 250px">
			<!-- List Content -->
			<cms:list/>
		</div>
	</div>
</document>
