<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Header -->
	<table class="list" data-height="300">
		<thead>
			<tr>
				<td style="width: 300px;">
					<cms:text constant="column_name" module="links"/>
				</td>
				<td style="width: 100px;">
					<cms:text constant="column_progress" module="links"/>
				</td>
				<td style="width: 70px;">
					<cms:text constant="column_clicks" module="links"/>
				</td>
				<td>
					<cms:text constant="column_options" module="links"/>
				</td>
			</tr>
		</thead>

		<tbody>
			<cms:link_list sponsored="1" template="overview_list_item.xml" local="1"/>
		</tbody>
	</table>
</document>
