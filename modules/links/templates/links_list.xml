<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<nav class="window">
		<cms:var param="link_new"/>
		<cms:var param="link_groups"/>
		<cms:var param="link_overview"/>
	</nav>

	<!-- Header -->
	<table class="list" data-height="400">
		<thead>
			<tr>
				<td style="width: 250px;">
					<cms:text constant="column_text" module="links"/>
				</td>
				<td style="width: 70px; text-align: center;">
					<cms:text constant="column_external" module="links"/>
				</td>
				<td style="width: 70px; text-align: center;">
					<cms:text constant="column_sponsored" module="links"/>
				</td>
				<td style="width: 90px; text-align: center;">
					<cms:text constant="column_total_clicks" module="links"/>
				</td>
				<td>
					<cms:text constant="column_options"/>
				</td>
			</tr>
		</thead>

		<tbody>
			<cms:link_list/>
		</tbody>
	</table>
</document>
