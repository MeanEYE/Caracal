<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<tr>
		<td>
			<cms:var param="text" multilanguage="yes"/>
		</td>
		<td>
			<div class="progress_bar" style="width: 90px;">
				<div
					class="gauge"
					style="'width: '.$params['display_percent'].'%'"
					cms:eval="style"
					/>
			</div>
		</td>
		<td>
			<cms:var param="sponsored_clicks"/>
			<cms:raw text=" / "/>
			<cms:var param="display_limit"/>
		</td>
		<td class="options">
			<cms:var param="item_change"/>
		</td>
	</tr>
</document>
