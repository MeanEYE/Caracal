<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!-- Window Menu -->
	<nav class="window">
		<cms:var name="$params['link_new']"/>
		<cms:var name="$params['link_templates']"/>
	</nav>

	<table class="list" data-height="300">
		<!-- Header -->
		<thead>
			<tr>
				<td style="width: 200px;">
					<cms:text constant="column_users_fullname" module="backend"/>
				</td>
				<td style="width: 150px;">
					<cms:text constant="column_users_username" module="backend"/>
				</td>
				<td style="width: 50px; text-align: center;">
					<cms:text constant="column_users_level" module="backend"/>
				</td>
				<td style="width: 60px; text-align: center;">
					<cms:text constant="column_users_verified" module="backend"/>
				</td>
				<td style="width: 60px; text-align: center;">
					<cms:text constant="column_users_agreed" module="backend"/>
				</td>
				<td>
					<cms:text constant="column_options"/>
				</td>
			</tr>
		</thead>

		<!-- List Content -->
		<tbody>
			<cms:user_list/>
		</tbody>
	</table>
</document>
