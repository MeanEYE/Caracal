<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<!--
		 Template used by the interactive embed script.
	-->
	<a
		id="$params['player_id']"
		class="youtube video interactive"
		href="$params['outside_url']"
		data-embed-url="$params['url']"
		data-width="$params['width']"
		data-height="$params['height']"
		data-fullscreen="$params['fullscreen']"
		cms:eval="href,data-embed-url,data-width,data-height,data-fullscreen"
		cms:optional="id"
		>
		<cms:thumbnail
			id="$params['id']"
			image_number="$params['image_number']"
			width="$params['width']"
			height="$params['height']"
			cms:eval="id,image_number,width,height"
			/>
	</a>
</document>
