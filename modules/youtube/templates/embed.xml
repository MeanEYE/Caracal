<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../../../units/schema/template.dtd">
<document>
	<iframe
		id="$params['player_id']"
		class="video"
		width="$params['width']"
		height="$params['height']"
		frameborder="_STANDARD != 'html5' ? 0 : false"
		src="$params['url']"
		allowfullscreen="allowfullscreen"
		cms:optional="frameborder,id"
		cms:eval="width,height,src"
		/>
</document>
